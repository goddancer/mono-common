{
  "presets": [
		[
			"@babel/preset-env",
			{
				"modules": "auto",
				"useBuiltIns": "usage",
				"corejs": 3
			}
		],
		"@babel/preset-typescript"
  ],
  "plugins": [
		"@babel/transform-runtime",
		"@babel/plugin-proposal-optional-chaining",
    "@babel/plugin-proposal-nullish-coalescing-operator",
		"@babel/plugin-transform-typescript"
  ],
  "comments": false,
  "overrides": [
    {
      "ignore": [
        "./common-module/authenticate/sjcl.js",
        "./common-module/applinks/*.js",
        "./common-module/detector.js",
        "**/node_modules/*",
        "**/vendor/*"
      ]
    }
  ]
}
